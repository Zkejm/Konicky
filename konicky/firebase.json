{
  "hosting": {
    "public": "public",
    "rewrites": [
      {
        "source": "/app",
        "destination": "/app/index.html"
      },
      {
        "source": "/app/**",
        "destination": "/app/index.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
